<%- include("partials/header.ejs"); %>
<div class="container">
  <h1>Home</h1>
  <% if (locals.homeContent); { %>
  <p><%= homeContent %></p>
  <% } %> <% if(locals.posts) { %> <% posts.forEach((post) => { %>
  <h1><%= post.title %></h1>
  <p>
    <%= post.content.substring(0, 10) + "..." %>
    <a href="posts/<%= post.title %>">Read more</a>
  </p>
  <% }); } %>
</div>
<%- include("partials/footer.ejs"); %>
